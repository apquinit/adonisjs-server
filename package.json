{
  "name": "template-adonisjs",
  "version": "0.0.0",
  "adonis-version": "4.1.0",
  "description": "AdonisJs template for twala projects.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "test": "node ace test",
    "start": "node server.js",
    "heroku:migrate": "ENV_SILENT=true node ace migration:run --force && ENV_SILENT=true node ace seed --force",
    "heroku:start": "ENV_SILENT=true npm start"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "Alexander Paul P. Quinit",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/vow": "^1.0.17"
  },
  "devDependencies": {
    "eslint": "^6.7.2"
  },
  "autoload": {
    "App": "./app"
  }
}
